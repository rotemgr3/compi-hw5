call i32 (i8*, ...) @printf(i8* getelementptr([4 x i8], [4 x i8]* @.int_internal, i32 0, i32 0), i32 %0)
ret void
}
define void @testRecursion(i32){
%var_2 = alloca i32, i32 50
%var_3 = add i32 10, 0
